type User {
    id: ID!
    email: String!
    groups(ownedBy: Boolean): [Group!]
    meetings(hostedBy: Boolean): [Meeting!]
}

type Group {
    id: ID!
    owner: User
    title: String!
    description: String
    members: [User!]
    meetings: [Meeting!]
}

type Meeting {
    id: ID!
    name: String!
    host: User
    group: Group
    dateTime: Int!
    attendees: [User!]
    type: MeetingType!
    location: String!
}

enum MeetingType {
    PHYSICAL
    ONLINE
}
