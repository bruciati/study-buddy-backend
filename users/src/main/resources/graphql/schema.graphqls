type Query {
    # List of "User" type
    users(ids: [ID!]): [User]

    # Single "User" type
    userById(id: ID!): User
    userByEmail(email: String!): User
}

type Group {
    id: ID!
    owner: User
    title: String!
    description: String
    members: [User!]
}

type User {
    id: ID!
    email: String!
    groups: [Group!]
}
